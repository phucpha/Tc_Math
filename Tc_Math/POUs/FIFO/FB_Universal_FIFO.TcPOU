    <Property Name="IsFull" Id="{9929d9b6-0d6d-4373-a85e-ad81b1b563c1}">
      <Declaration><![CDATA[PROPERTY IsFull : bool]]></Declaration>
      <Get Name="Get" Id="{de385dae-c82c-49f8-8b9e-9ac114441ac0}">
