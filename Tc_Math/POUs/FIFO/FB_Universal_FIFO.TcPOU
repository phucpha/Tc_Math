    <Property Name="IsEmpty" Id="{6ac4853d-dd4b-4eab-ae86-df148cbd7815}">
      <Declaration><![CDATA[PROPERTY IsEmpty : bool]]></Declaration>
      <Get Name="Get" Id="{d3b4f330-ad27-4609-8b4b-bce8d522bd59}">
