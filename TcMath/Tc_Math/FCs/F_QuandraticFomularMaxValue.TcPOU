  <POU Name="F_QuandraticFomularMaxValue" Id="{8e68f864-77a1-4311-bcb3-bee7a62ed476}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION F_QuandraticFomularMaxValue : LREAL 
VAR_INPUT
	A : LREAL;
	B : LREAL;
	C : LREAL;
	xLimit1 : LREAL;
	xLimit2 : LREAL;
END_VAR
VAR
	yAtxLimit1 : LREAL;
	yAtxLimit2 : LREAL;
	Adot : LREAL; // Adot = A'
	Bdot : LREAL; // Bdot = B'
	x : LREAL;
END_VAR
VAR_OUTPUT
	MaxValue : LREAL;
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[yAtxLimit1 := F_QuanDraticCalculate(
	A := A,
	B := B,
	C := C,
	x := xLimit1
);
yAtxLimit2 := F_QuanDraticCalculate(
	A := A,
	B := B,
	C := C,
	x := xLimit2
);		
IF A > 0 THEN
	MaxValue := MAX(yAtxLimit1, yAtxLimit2);
ELSIF A < 0 THEN
	Adot := 2 * A;
	Bdot := B;
	x := -Bdot / Adot;
	MaxValue := F_QuanDraticCalculate(
		A := A,
		B := B,
		C := C,
		x := x
	);	
ELSE // A = 0, b is any
	MaxValue := MAX(yAtxLimit1, yAtxLimit2);
END_IF
F_QuandraticFomularMaxValue := MaxValue;]]></ST>
    </Implementation>
